{
  "name": "@vitorluizc/maybe",
  "version": "2.0.0",
  "description": "ðŸŒ¯ The prettiest TypeScript implementation of Maybe monad.",
  "cdn": "dist/maybe.umd.js",
  "types": "dist/maybe.d.ts",
  "main": "dist/maybe.js",
  "unpkg": "dist/maybe.umd.js",
  "module": "dist/maybe.mjs",
  "jsdelivr": "dist/maybe.umd.js",
  "umd:main": "dist/maybe.umd.js",
  "files": ["dist/"],
  "scripts": {
    "docs": "typedoc",
    "test": "ava",
    "build": "bili",
    "prepare": "yarn docs && yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/maybe.git"
  },
  "keywords": [
    "maybe",
    "maybe-type",
    "maybe-monad",
    "optional",
    "optional-type",
    "optional-typing",
    "functional-programming",
    "functional",
    "fp",
    "type-safety",
    "type-safe",
    "typescript",
    "bili",
    "ava"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/maybe/issues"
  },
  "homepage": "https://github.com/VitorLuizC/maybe#readme",
  "devDependencies": {
    "ava": "^1.2.1",
    "bili": "^4.2.5",
    "rollup-plugin-typescript2": "^0.19.2",
    "ts-node": "^8.0.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.27",
    "typescript": "^3.3.3"
  },
  "ava": {
    "require": ["ts-node/register"],
    "extensions": ["ts"],
    "compileEnhancements": false
  },
  "bili": {
    "input": "src/maybe.ts",
    "output": {
      "format": ["es", "cjs", "umd", "umd-min"],
      "moduleName": "Maybe"
    },
    "plugins": {
      "typescript2": {
        "clean": true,
        "useTsconfigDeclarationDir": true
      }
    }
  }
}
