{
  "name": "@vitorluizc/maybe",
  "version": "2.0.0",
  "description": "ðŸŒ¯ The prettiest TypeScript implementation of Maybe monad.",
  "cdn": "dist/maybe.umd.js",
  "types": "dist/maybe.d.ts",
  "main": "dist/maybe.js",
  "unpkg": "dist/maybe.umd.js",
  "module": "dist/maybe.esm.js",
  "jsdelivr": "dist/maybe.umd.js",
  "umd:main": "dist/maybe.umd.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bili",
    "prepare": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/maybe.git"
  },
  "keywords": [
    "maybe",
    "maybe-type",
    "maybe-monad",
    "optional",
    "optional-type",
    "optional-typing",
    "functional-programming",
    "functional",
    "fp",
    "type-safety",
    "type-safe",
    "typescript",
    "bili",
    "ava"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/maybe/issues"
  },
  "homepage": "https://github.com/VitorLuizC/maybe#readme",
  "devDependencies": {
    "bili": "^4.4.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "typescript": "^3.3.3333"
  },
  "bili": {
    "input": "src/main.ts",
    "output": {
      "format": [
        "es",
        "cjs",
        "umd",
        "umd-min"
      ],
      "moduleName": "Maybe"
    },
    "plugins": {
      "typescript2": {
        "clean": true,
        "useTsconfigDeclarationDir": true
      }
    }
  },
  "dependencies": {
    "@bitty/falsy": "workspace:*",
    "@bitty/nullish": "workspace:*",
    "@bitty/predicate": "workspace:*"
  }
}
